{
  "version": 2,
  "name": "copa-tomatao",
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/client/index.html"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm run vercel:build",
  "outputDirectory": "client/dist"
}
